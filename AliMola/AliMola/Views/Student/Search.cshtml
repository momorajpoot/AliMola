@model aliMola.Models.Student

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>


@using (Html.BeginForm("searchResult", "Student"))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Student</h4>
        <hr />
        @Html.ValidationSummary(true)

        <div class="form-group col-md-5">
            @Html.LabelFor(model => model.Roll_Num, new { @class = "control-label col-md-4" })
            <div class="col-md-8">
                @Html.EditorFor(model => model.Roll_Num)
            </div>
        </div>


        <div class="form-group col-md-2 ">
            <input type="submit" value="Search" class="btn btn-default" />
        </div>
    </div>
        <div class="clearfix"></div>

}






